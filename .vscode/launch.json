"version": "0.2.0",
  "configurations": [
    {
      "name": "STM32F401RE (Cortex-Debug / OpenOCD)",
      "type": "cortex-debug",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "executable": "${workspaceFolder}/build/blink.elf",
      "servertype": "openocd",
      "serverpath": "/usr/bin/openocd",
      "configFiles": [
        "/usr/share/openocd/scripts/interface/stlink.cfg",
        "/usr/share/openocd/scripts/target/stm32f4x.cfg"
      ],
      "runToEntryPoint": "main",
      "gdbPath": "/usr/bin/gdb-multiarch"
    }
  ]
}